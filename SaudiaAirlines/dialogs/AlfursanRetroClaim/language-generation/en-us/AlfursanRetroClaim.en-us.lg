      "errorMessage": "Please enter a valid 6-digit booking reference, for example: 67AXXX"
    },
    {
      "type": "Input.Text",
      "id": "pnr",
      "label": "Enter your ticket number",
      "isRequired": true,
      "iconUrl": "https://adaptivecards.io/content/send.png",
      "regex":"^[0-9]{3}[-]{1}[0-9]{10}$",
      "errorMessage": "Please enter a valid 13-digit ticket number, for example: 065-23XXXXXXXX"
    },
    {
      "type": "Input.Date",
      "id": "depart_date",
      "label": "Enter your departure date",
      "isRequired": true,
      "errorMessage": "Please enter a valid date, for example: 08/22/2023"
    },
    {
      "type": "Input.Text",
      "id": "orgin",
      "label": "Enter your origin",
      "isRequired": true,
      "regex":"^[A-Z]+$",
      "errorMessage": "Please enter a valid orgin, for example: JED"
    },
    {
      "type": "Input.Text",
      "id": "destination",
      "label": "Enter your destination",
      "isRequired": true,
      "regex":"^[A-Z]+$",
      "errorMessage": "Please enter a valid destination, for example: RUH"
    },
    {
      "type": "Input.Text",
      "id": "airline_code",
      "label": "Enter your airline code",
      "isRequired": true,
      "regex":"^[A-Z]{2}$",
      "errorMessage":  "Please enter a valid airline code, for example: SV"
    },
    {
      "type": "Input.Text",
      "id": "flight_number",
      "label": "Enter your flight number",
